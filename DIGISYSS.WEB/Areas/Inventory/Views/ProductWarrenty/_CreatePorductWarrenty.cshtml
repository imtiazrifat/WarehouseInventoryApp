
@*<script src="~/Areas/Inventory/Scripts/InvProductWarrenty/CreateProductWarrenty.js"></script>*@

<script src="~/Areas/Inventory/Scripts/ProductWarrenty/CreateProductWarrenty.js"></script>

<div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">APT ERP</h4>
            </div>
            <div id="modal-body">
                <h2 id="rif"></h2>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-warning" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>


<div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false" data-widget-custombutton="false">
    <header>
        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
        <h2>Create ProductWarrenty </h2>

    </header>

    <!-- widget div-->
    <div>

        <!-- widget edit box -->
        <div class="jarviswidget-editbox">
            <!-- This area used as dropdown edit box -->

        </div>
        <!-- end widget edit box -->
        <!-- widget content -->
        <div class="widget-body no-padding">

            <div class="smart-form">
                <fieldset>
                    <input type="text" id="hdnProductWarrentyId" hidden="" value="0" />

                    <section>
                        <label for="txtProductId" class="label">Product Id</label>
                        <label class="input">
                            <input type="text" id="txtProductId" name="txtProductId" maxlength="50" placeholder="Product Warrenty Id" required />
                        </label>
                    </section>

                    <section>
                        <label class="checkbox">
                            <input type="checkbox" name="chkIsPlacementWarrenty" id="chkIsReplacementWarrenty">
                            <i></i>Is Placement Warrenty

                        </label>
                    </section>

                    <section>
                        <label for="txtWarrentyPeriodId" class="label">Warrenty Period Id</label>
                        <label class="input">
                            <input type="text" id="txtWarrentyPeriodId" name="txtWarrentyPeriodId" maxlength="50" placeholder="Warrenty Period Id" />
                        </label>
                    </section>
                    <section>
                        <label for="txtWarrentyExpireDate" class="label">Expire Date</label>
                        <label class="input">
                            <input type="datetime" id="txtWarrentyExpireDate" name="txtWarrentyExpireDate" maxlength="50" placeholder="Expire Date" />
                        </label>
                    </section>
                    
                    <section>
                        <label for="txtServicePeriod" class="label">Service Period</label>
                        <label class="input">
                            <input type="text" id="txtServicePeriod" name="txtServicePeriod" maxlength="50" placeholder="Service Period" />
                        </label>
                    </section>
                    <section>
                        <label for="txtWarrentyDescription" class="label">Warrenty Description</label>
                        <label class="input">
                            <input type="text" id="txtWarrentyDescription" name="txtWarrentyDescription" maxlength="350" placeholder="Warrenty Description" />
                        </label>
                    </section>
                    <section>
                        <label for="txtEx1" class="label">Extra1</label>
                        <label class="input">
                            <input type="text" id="txtEx1" name="txtEx1" maxlength="50" placeholder="Extra1" />
                        </label>
                    </section>

                    @*<section>
                            <label for="chkIsActive" class="label">Is Active</label>
                            <label class="checkbox">
                                <input type="checkbox" id="chkIsActive" name="IsActive" />
                            </label>

                        </section>*@
                    <section>
                        <label class="checkbox">
                            <input type="checkbox" name="chkIsActive" id="chkIsActive">
                            <i></i>IsActive

                        </label>

                    </section>
                </fieldset>

                <footer>
                    <button class="btn btn-primary" id="btnSaveProductWarrenty">
                        @*onclick="createAssetProductWarrentyManager.SaveAssetProductWarrenty()"*@
                        Save
                    </button>
                    <button type="button" class="btn btn-warning" id="btnClearProductWarrentyForm">
                        Clear
                    </button>
                </footer>
            </div>

        </div>
        <!-- end widget content -->

    </div>
    <!-- end widget div -->

</div>
<!-- end widget -->


<div id="dialog_simple" title="APT ERP V 1.0.0">
    <p>
    </p>
</div>

