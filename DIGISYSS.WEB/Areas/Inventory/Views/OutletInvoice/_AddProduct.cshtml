<div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">APT ERP</h4>
            </div>
            <div id="modal-body">
                <h2 id="rif"></h2>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-warning" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<head>
    @*tsdstdstd*@
</head>

<div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false" data-widget-custombutton="false">
    <header>
        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
        <h2>Add Product</h2>

    </header>

    <!-- widget div-->
    <div>

        <!-- widget edit box -->
        <div class="jarviswidget-editbox">
            <!-- This area used as dropdown edit box -->

        </div>
        <!-- end widget edit box -->
        <!-- widget content -->
        <div class="widget-body no-padding">

            <div class="smart-form">
                <div data-toggle="validator">
                    <div class="container-fluid">
                        <input type="text" id="hdnOutletInvoiceMasterId" hidden="" value="0" />
                        <input type="text" id="hdnOutletInvoiceDetailsId" hidden="" value="0" />
                        <div class="row-xs-12">
                            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                                <fieldset>
                                    <div class="row-xs-12">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <label for="txtOutletName" class="label">Outlet Name</label>
                                                    <label class="input">
                                                        <input type="text" id="txtOutletName" name="txtOutletName" maxlength="50" placeholder="Branch Name" readonly="readonly" />
                                                    </label>
                                                </section>
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <label for="txtSalePerson" class="label">Sale Person </label>

                                                    <label class="input">
                                                        <input type="text" id="txtSalePerson" name="txtSalePerson" maxlength="50" placeholder="Sale Person" readonly="readonly" />
                                                    </label>
                                                </section>
                                            </div>

                                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <label for="txtOutletSaleInvoiceNo" class="label">Invoice No</label>

                                                    <label class="input">
                                                        <input type="number" id="txtOutletSaleInvoiceNo" name="txtOutletSaleInvoiceNo" maxlength="50" placeholder="Invoice No" readonly="readonly" />
                                                    </label>
                                                </section>
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <label for="txtCustomerName" class="label">Customer Name</label>
                                                    <label class="input">
                                                        <input type="text" id="txtCustomerName" name="txtCustomerName" maxlength="50" placeholder="Customer Name" />
                                                    </label>
                                                </section>
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <h6>Current Stock</h6>
                                                    <label for="txtCurrentStock" class="label"></label>
                                                    <label class="input">
                                                        <input type="number" id="txtCurrentStock" name="txtCurrentStock" maxlength="50" placeholder="Current Stock" readonly="readonly" />
                                                    </label>
                                                </section>
                                            </div>
                                        </div>

                                        <div class="row-xs-12">
                                            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <h6>Product</h6>
                                                    <label for="cmbProductId" class="label"></label>
                                                    <input type="text" id="hdnProductId" hidden="" />
                                                    <input type="text" id="hdnProductName" hidden="" />
                                                    <label class="select">
                                                        <select id="cmbProductId"><option></option></select>
                                                    </label>
                                                </section>
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <h6>Quantity</h6>
                                                    <label for="txtProductQuantity" class="label"></label>
                                                    <label class="input">
                                                        <input type="number" id="txtProductQuantity" name="txtProductQuantity" maxlength="50" placeholder="Quantity" min="1" step="1" />
                                                    </label>
                                                </section>
                                            </div>
                                            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12" style="padding-right: 10px">
                                                <section>
                                                    <h6>Price</h6>
                                                    <label for="txtPriceAmount" class="label"></label>
                                                    <label class="input">
                                                        <input type="number" id="txtPriceAmount" name="txtPriceAmount" maxlength="50" placeholder="Price" readonly="readonly" />
                                                    </label>
                                                </section>
                                            </div>
                                            @*<div class="col-lg-3 col-md-3 col-sm-3 col-xs-12" style="padding-bottom: 10px">
                                                <footer>
                                                    <div class="col-lg-12">
                                                        <button class="btn btn-primary" id="btnAddProduct"><span class="glyphicon glyphicon-shopping-cart"></span> Add Product</button>
                                                    </div>
                                                </footer>
                                            </div>*@
                                        </div>

                                    </div>
                                </fieldset>

                            </div>
                            <div class="row-lg-8">
                                <div class="col-sm-9">
                                    <fieldset>
                                        <footer>
                                            <button class="btn btn-primary" id="btnAddProduct"><span class="glyphicon glyphicon-shopping-cart"></span> Add Product</button>
                                            <button type="button" class="btn btn-warning" id="btnClearOutletSaleUIForm">Clear</button>
                                        </footer>
                                    </fieldset>
                                </div>
                            </div>




                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                                <fieldset>
                                    <div class="row-xs-8">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            <div class="col-xs-1" style="visibility: hidden">
                                                <section>
                                                    <label class="input">
                                                        <input type="number" id="txtRetailPrice" name="txtRetailPrice" />
                                                    </label>
                                                </section>
                                                <section>
                                                    <label class="input">
                                                        <input type="number" id="txtTotalOutletSaleUIPrice" name="txtTotalOutletSaleUIPrice" />
                                                    </label>
                                                </section>
                                                <section>
                                                    <label class="input">
                                                        <input type="number" id="txtTotalProduct" name="txtTotalProduct" />
                                                    </label>
                                                </section>
                                            </div>
                                        </div>
                                    </div>

                                </fieldset>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!-- end widget content -->
    <!-- end widget div -->

</div>
<!-- end widget -->


